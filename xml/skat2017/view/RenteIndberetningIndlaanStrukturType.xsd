	<xs:element name="RenteIndberetningIndlånStruktur" type="ekap2017:RenteIndberetningIndlånStrukturType"/>
	<xs:complexType name="RenteIndberetningIndlånStrukturType">
		<xs:sequence>
			<xs:element name="Indberetningsperiode">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ekap2017:IndkomstÅr"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Indberetningspligtig">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ekap2017:VirksomhedSENummer"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ekap2017:KontoID"/>
			<xs:element name="IndberetningValg">
				<xs:complexType>
					<xs:choice>
						<xs:sequence>
							<xs:element name="Invalidering">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ekap2017:IndberetningIdentifikatorStruktur"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:sequence>
							<xs:element name="RetbarKontoOplysninger">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ekap2017:IndberetningIdentifikatorStruktur"/>
										<xs:element ref="ekap2017:NoteTekst" minOccurs="0"/>
										<xs:element name="KontoDetaljer">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Pengeinstitut" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="ekap2017:BankRegistreringsnummer"/>
																<xs:element ref="ekap2017:BankKontoIBANNummer"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element ref="ekap2017:IndlånKontoNummer"/>
													<xs:element ref="ekap2017:KontoStartDato"/>
													<xs:element ref="ekap2017:KontoOphørDato" minOccurs="0"/>
													<xs:element ref="ekap2017:KontoValutaKode" minOccurs="0"/>
													<xs:element ref="ekap2017:IndlånKontoInddragelseStatus" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="Beløb" minOccurs="0">
											<xs:complexType>
												<xs:sequence>
													<xs:element ref="ekap2017:RenteBeløb" minOccurs="0"/>
													<xs:element ref="ekap2017:IndlånKontoIndestående" minOccurs="0"/>
													<xs:element ref="ekap2017:RenteKalkuleretPræmieBeløb" minOccurs="0"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="KontoperiodeÅrsUltimoListe">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Kontoperiode" minOccurs="1" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="ekap2017:RenteBeløb" minOccurs="0"/>
																<xs:element ref="ekap2017:KontoperiodeOpstartDato" minOccurs="0"/>
																<xs:element ref="ekap2017:KontoperiodeOphørDato" minOccurs="0"/>
																<xs:element ref="ekap2017:KontoTypeKode"/>
																<xs:element ref="ekap2017:FlereEndToKontohavereIkkeAlleOplyst"/>
																<xs:element name="KontohaverListe">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="KontohaverOplysninger" minOccurs="1" maxOccurs="unbounded">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="KontohaverValg">
																							<xs:complexType>
																								<xs:choice>
																									<xs:sequence>
																										<xs:element name="IndlændingValg">
																											<xs:complexType>
																												<xs:choice>
																													<xs:sequence>
																														<xs:element name="PersonValg">
																															<xs:complexType>
																																<xs:choice>
																																	<xs:element ref="ekap2017:PersonCPRNummer"/>
																																	<xs:sequence>
																																		<xs:element name="KontohaverUdenCPRNummer">
																																			<xs:complexType>
																																				<xs:sequence>
																																					<xs:element ref="ekap2017:PersonNavnForNavn"/>
																																					<xs:element ref="ekap2017:PersonNavnEfterNavn"/>
																																					<xs:element ref="ekap2017:FødselInformationStruktur" minOccurs="0"/>
																																					<xs:element ref="ekap2017:IndberetningDanskAdresseStruktur" minOccurs="0"/>
																																				</xs:sequence>
																																			</xs:complexType>
																																		</xs:element>
																																	</xs:sequence>
																																</xs:choice>
																															</xs:complexType>
																														</xs:element>
																													</xs:sequence>
																													<xs:element ref="ekap2017:VirksomhedSENummer"/>
																												</xs:choice>
																											</xs:complexType>
																										</xs:element>
																									</xs:sequence>
																									<xs:sequence>
																										<xs:element name="ValutaudlændingValg">
																											<xs:complexType>
																												<xs:choice>
																													<xs:sequence>
																														<xs:element name="Person">
																															<xs:complexType>
																																<xs:sequence>
																																	<xs:element ref="ekap2017:PersonCPRNummer" minOccurs="0"/>
																																	<xs:element ref="ekap2017:PersonNavnForNavn"/>
																																	<xs:element ref="ekap2017:PersonNavnEfterNavn"/>
																																	<xs:element ref="ekap2017:IndberetningUdenlandskAdresseStruktur" minOccurs="0"/>
																																	<xs:element ref="ekap2017:FødselInformationStruktur" minOccurs="0"/>
																																	<xs:element ref="ekap2017:TINNummerStruktur" minOccurs="0"/>
																																	<xs:element ref="ekap2017:VirksomhedFormandAdministrator"/>
																																	<xs:element ref="ekap2017:KontohaverIdentifikationMulig"/>
																																</xs:sequence>
																															</xs:complexType>
																														</xs:element>
																													</xs:sequence>
																													<xs:sequence>
																														<xs:element name="Virksomhed">
																															<xs:complexType>
																																<xs:sequence>
																																	<xs:element ref="ekap2017:VirksomhedSENummer" minOccurs="0"/>
																																	<xs:element ref="ekap2017:VirksomhedNavnFirmaNavn"/>
																																	<xs:element ref="ekap2017:IndberetningUdenlandskAdresseStruktur" minOccurs="0"/>
																																	<xs:element ref="ekap2017:TINNummerStruktur" minOccurs="0"/>
																																</xs:sequence>
																															</xs:complexType>
																														</xs:element>
																													</xs:sequence>
																												</xs:choice>
																											</xs:complexType>
																										</xs:element>
																									</xs:sequence>
																									<xs:element ref="ekap2017:KontohaverUkendt"/>
																									<xs:element ref="ekap2017:KontohaverDiplomat"/>
																								</xs:choice>
																							</xs:complexType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
